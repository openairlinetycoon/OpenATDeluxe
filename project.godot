; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="OpenATD"
run/main_scene="res://scenes/load.tscn"
config/project_settings_override="settings.cfg"
config/icon="res://icon.png"
config/atd_path="P:/Projekte/Major Games/BASE/ATD"

[audio]

bus2vol=-4
bus6vol=-18
bus5vol=-32
bus3vol=-25
bus4vol=-4

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "builder", "midi" )

[graphics]

transparency=true
passenger=false
shadow=false
thoughts=true
blind=true
roomDetails=false
scrolling=true
planes=false

[locale]

translations=PoolStringArray( "res://localization/localization.de.translation", "res://localization/localization.en.translation", "res://localization/localization.es.translation", "res://localization/localization.fr.translation", "res://localization/localization.it.translation", "res://localization/localization.nl.translation", "res://localization/localization.pt.translation", "res://localization/localization.pt_BR.translation" )
locale_filter=[ 1, [  ] ]

[mono]

unhandled_exception_policy=1
profiler/args="default:alloc,output=output.mlpd"

[other]

secretary=true
advisor=false
autosave=true
fax=false
realNames=true
textBubbles=false
summary=true
gameStartToday=true
roundNumbers=false

[rendering]

environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"
